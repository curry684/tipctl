{
    "name": "transip/api.cli.client",
    "description": "TransIP REST Api CLI client",
    "type": "project",
    "authors": [
        {
            "name": "Stefan Konig",
            "email": "skonig@transip.nl"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "symfony/console": "4.3.*",
        "symfony/finder": "4.3.*",
        "transip/api.client": "@dev",
        "ext-json": "*",
        "symfony/yaml": "4.3.*"
    },
    "autoload": {
        "psr-4": {
            "Transip\\Api\\CLI\\": "src/Transip/Api/CLI"
        }
    },
    "repositories": {
        "dev-package": {
            "type": "path",
            "url": "../api.client",
            "options": {
                "symlink": true
            }
        }
    }
}
